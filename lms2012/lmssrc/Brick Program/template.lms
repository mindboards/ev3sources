  MOVE32_32(0,Time)                                                   //    Time    =  0
                                                                      //
  JR_NEQ16(Par1,1,Not1)                                               //    if (Par1 == 1)
                                                                      //    {
  MOVE32_32(250,Time)                                                 //      Time  =  250
                                                                      //
