// 8 SOUNDBLOCK ****************************************************************************************
                                                                      //
subcall   SoundBlock                                                  //  void SoundBlock(Par1,Par2)
{                                                                     //  {
  IN_16   Par1                                                        //
  IN_16   Par2                                                        //
                                                                      //
  DATA16  Sound                                                       //
  DATA8   Tmp                                                         //
  DATA8   Flag                                                        //
  DATA8   Volume                                                      //
  DATAS   Filename FILENAMESIZE                                       //
  DATAS   String FILENAMESIZE                                         //
                                                                      //
  SOUND(BREAK)                                                        //    SOUND(BREAK)
  JR_EQ16(Par1,1,StopSound)                                           //    if (Par1 != 1)
                                                                      //    {
  SUB16(Par1,1,Sound)                                                 //      Sound  =  Par1 - 1
