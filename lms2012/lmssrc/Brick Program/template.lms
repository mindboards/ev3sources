subcall   DelayBlock                                                  //  void DelayBlock()
{                                                                     //  {
  IN_16   Par1                                                        //
  IN_16   Par2                                                        //
                                                                      //
  DATA32  Timer                                                       //
  DATA32  Time                                                        //
