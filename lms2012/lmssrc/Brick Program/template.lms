// 13 COLORBLOCK ***************************************************************************************
                                                                      //
subcall   COLORBlock                                                  //  void ColorBlock(Par1,Par2)
{                                                                     //  {
  IN_16   Par1                                                        //
  IN_16   Par2                                                        //
                                                                      //
  DATAF   Value                                                       //
  DATA16  Color                                                       //
